{
	"name": "example-blocks",
	"version": "0.1.0",
	"description": "Collection of example blocks.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "run-p build:*",
		"build:hello-world": "wp-scripts build blocks/hello-world/src/index.js --output-path=blocks/hello-world/build/",
		"build:reverse-string": "wp-scripts build blocks/reverse-string/src/index.js --output-path=blocks/reverse-string/build/",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "run-p start:*",
		"start:hello-world": "wp-scripts start blocks/hello-world/src/index.js --output-path=blocks/hello-world/build/",
		"start:reverse-string": "wp-scripts start blocks/reverse-string/src/index.js --output-path=blocks/reverse-string/build/"
	},
	"devDependencies": {
		"@wordpress/scripts": "^26.13.0",
		"npm-run-all": "^4.1.5"
	}
}
